<h1>Edit material</h1>

<br><br><br>
<body class="universal">
    
<div class="form">
<%= form_for(@material) do |f|%> <!--, :html => {:multipart => true}) -->
    <%= f.label "material Name: ", :for => "email", :class => "form__label" %>
    <%= f.text_field :title, :required => true, :id => "email", :class => "form__input", :type => "text" %>
</div>

<div class="form">
    <%= f.submit "Edit material"%>
</div>
<% end %>

</body>